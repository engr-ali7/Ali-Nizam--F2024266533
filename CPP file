void ratings()
{
	int rating;
	cout<<"Rate our product"<<endl;
	cin>>rating;
	while(rating<1 || rating>5){
	cout<<"Enter between 1 to 5"<<endl;
	cin>>rating;
	}
	cout<<"Thanks for rating our product."<<endl;
	for(int i=1;i<=rating;i++)
	cout<<"*";
}










